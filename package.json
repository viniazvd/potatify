{
  "name": "potatify",
  "version": "0.1.25",
  "type": "module",
  "keywords": [
    "vue3",
    "vue",
    "typescript",
    "vitest",
    "storybook",
    "pinia",
    "vue-router",
    "boilerplate"
  ],
  "scripts": {
    "dev": "vite",
    "build:css": "cross-env NODE_ENV=production tailwindcss -o dist/tailwind.css -m",
    "build": "vite build && vue-tsc --emitDeclarationOnly && yarn build:css",
    "preview": "vite preview",
    "test": "vitest --watch --ui",
    "coverage": "vitest run --coverage",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 3000 --no-open"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/potatify.umd.js",
  "module": "./dist/potatify.es.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    ".": {
      "import": "./dist/potatify.es.js",
      "require": "./dist/potatify.umd.js"
    }
  },
  "peerDependencies": {
    "tailwindcss": "^3.2.1"
  },
  "types": "./dist/types/index.d.ts",
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.3",
    "@vueuse/core": "^9.4.0",
    "animate.css": "^4.1.1",
    "pinia": "^2.0.22",
    "utils-sdz": "^0.0.12",
    "uuid": "^9.0.0",
    "vue": "^3.2.37",
    "vue-coerousel": "^1.0.19",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@iconify/vue": "^4.0.0",
    "@pinia/testing": "^0.0.14",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.12",
    "@tailwindcss/typography": "^0.5.8",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/coverage-c8": "^0.24.0",
    "@vitest/ui": "^0.24.0",
    "@vue/test-utils": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "form-data": "^4.0.0",
    "jsdom": "^20.0.1",
    "postcss": "^8.4.18",
    "sass": "^1.55.0",
    "storybook-dark-mode": "^1.1.2",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-checker": "^0.5.1",
    "vite-tsconfig-paths": "^3.5.1",
    "vitest": "^0.24.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.40.4"
  }
}
